.slick-prev:before,
.slick-next:before
{
    font-family: 'techrev';
    line-height: 1;
    opacity: .75;
    color: black;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}
    



.slick-prev:before
{
    content: '\e905';
}
[dir='rtl'] .slick-prev:before
{
    content: '\e905';
}
.slick-next:before
{
    content: '\e906';
}
[dir='rtl'] .slick-next:before
{
    content: '\e905';
}
